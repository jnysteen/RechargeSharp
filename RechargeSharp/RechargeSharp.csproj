<Project Sdk="Microsoft.NET.Sdk">


  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>3.2.6</Version>
    <AssemblyVersion>3.2.6.0</AssemblyVersion>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/mattgenious/RechargeSharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mattgenious/RechargeSharp</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>C#, .NET, ecommerce, recharge, rechargepayments</PackageTags>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <FileVersion>3.2.6.0</FileVersion>
    <Authors>Matt Luccas Phaure Jensen</Authors>
    <Description>A C# library for RechargePayments</Description>
    <PackageReleaseNotes>Set retry rate to 3 seconds again</PackageReleaseNotes>
    <PackageIconUrl>https://user-images.githubusercontent.com/3007300/64613121-ab7b7380-d3d5-11e9-9703-126f3448863b.png</PackageIconUrl>
    <LangVersion>8.0</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Http" Version="3.1.2" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="NuGet.Versioning" Version="5.4.0" />
    <PackageReference Include="Polly" Version="7.2.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
