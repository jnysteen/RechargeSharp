<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.0.1</Version>
    <AssemblyVersion>0.0.1.0</AssemblyVersion>
    <PackageLicenseExpression>https://github.com/mattgenious/RechargeSharp/blob/master/LICENSE</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/mattgenious/RechargeSharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mattgenious/RechargeSharp</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>C#, .NET</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>

</Project>
