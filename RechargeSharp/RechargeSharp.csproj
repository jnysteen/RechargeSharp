<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.1.3</Version>
    <AssemblyVersion>0.1.3.0</AssemblyVersion>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/mattgenious/RechargeSharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mattgenious/RechargeSharp</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>C#, .NET, ecommerce, recharge, rechargepayments</PackageTags>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <FileVersion>0.1.3.0</FileVersion>
    <Authors>Matt Luccas Phaure Jensen</Authors>
    <Description>A C# library for RechargePayments</Description>
    <PackageReleaseNotes>Fixed error in retry logic not handling 429 errors from recharge server</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Http" Version="2.2.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Include="Polly" Version="7.1.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
